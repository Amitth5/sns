 
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css">


<%= JSON.stringify(data) %>

<div class="container">

    <div class="card text-white bg-primary mb-3" style="max-width: 18rem;">
        <div class="card-header"><h1><%= data.totalOrder %></h1>  </div>
        <div class="card-body">
          <h3 class="card-title">Total Orders Today</h3>
        </div>
      </div>
      <div class="card text-white bg-secondary mb-3" style="max-width: 18rem;float:left !important">
        <div class="card-header"><h1><%= data.totalOrderValue %></h1>  </div>
        <div class="card-body">
          <h3 class="card-title">Total Sales </h3>
        </div>
      </div>
      <div class="card text-white bg-success mb-3" style="max-width: 18rem; float:left !important">
        <div class="card-header"><h1><%= data.totalStoreOrderValue %></h1> </div>
        <div class="card-body">
          <h3 class="card-title">Total Stores Sales</h3>
        </div>
      </div>
      <div class="card text-white bg-danger mb-3" style="max-width: 18rem; float:left !important">
        <div class="card-header"> <h1><%= data.totalDeliveryCharge %></h1> </div>
        <div class="card-body">
          <h3 class="card-title">Total Delivery Charges</h3>
        </div>
      </div>
      <div class="card text-white bg-warning mb-3" style="max-width: 18rem;">
        <div class="card-header"><h2> Sunil Details</h2></div>
        <div class="card-body">
          <h3 class="card-title"><b> Total Orders </b>: <%= data.deliveryDetails[0]['orderCount'] %></h3>
        </div>
      </div>
      <div class="card text-white bg-info mb-3" style="max-width: 18rem;">
        <div class="card-header">Header</div>
        <div class="card-body">
          <h5 class="card-title">Info card title</h5>
          <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
        </div>
      </div>
      <div class="card bg-light mb-3" style="max-width: 18rem;">
        <div class="card-header">Header</div>
        <div class="card-body">
          <h5 class="card-title">Light card title</h5>
          <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
        </div>
      </div>
      <div class="card text-white bg-dark mb-3" style="max-width: 18rem;">
        <div class="card-header">Header</div>
        <div class="card-body">
          <h5 class="card-title">Dark card title</h5>
          <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
        </div>
      </div>
</div>